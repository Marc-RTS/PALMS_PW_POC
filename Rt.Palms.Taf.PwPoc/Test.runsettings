<?xml version="1.0" encoding="utf-8"?>
<RunSettings>
	<!-- NUnit adapter -->
	<NUnit>
		<NumberOfTestWorkers>14</NumberOfTestWorkers>
	</NUnit>
	<!-- General run configuration -->
	<RunConfiguration>
		<EnvironmentVariables>
			<!-- For debugging selectors, it's recommend to set the following environment variable -->
			<DEBUG>pw:api</DEBUG>
		</EnvironmentVariables>
	</RunConfiguration>
	<!-- Playwright -->
	<Playwright>
		<BrowserName>chromium</BrowserName>
		<ExpectTimeout>10000</ExpectTimeout>
		<LaunchOptions>
			<Headless>false</Headless>
			<Channel>msedge</Channel>
		</LaunchOptions>
	</Playwright>
	<TestRunParameters>
		<Parameter name="webUrl" value="https://yellow-coast-04525bd00.3.azurestaticapps.net/" />
		<Parameter name="screenshot" value="true" />
		<Parameter name="snapshots" value="true" />
		<Parameter name="sources" value="true" />
		<Parameter name="traces" value="playwright-traces" />
	</TestRunParameters>
</RunSettings>